main Sqrt /* square root of a given number */

   var
   a, x,delta: real;
   stop: int;

start
      read (a);
      x := a/2.0;
      stop := 0;
      
      while(stop == 0) {
         x := x/2.0+a/(2.0*x);
         delta :=  x*x-a;

         if(delta < 0) then { delta := 0 - delta;} else { }
         if (delta < 0.0001) then { stop := 1;} else { }
       }
       print (a);
       print (x);
trats
